<p id="notice"><%= notice %></p>

<h1>Listing Parcials</h1>

<table>
  <thead>
    <tr>
      <th>Numero</th>
      <th>Calificacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parcials.each do |parcial| %>
      <tr>
        <td><%= parcial.numero %></td>
        <td><%= parcial.calificacion %></td>
        <td><%= link_to 'Show', parcial %></td>
        <td><%= link_to 'Edit', edit_parcial_path(parcial) %></td>
        <td><%= link_to 'Destroy', parcial, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parcial', new_parcial_path %>
